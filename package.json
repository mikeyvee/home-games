{
  "name": "home-games",
  "version": "0.0.1",
  "description": "Games for your local network",
  "dependencies": {
    "babel": "5.0.12",
    "body-parser": "1.12.2",
    "cookie": "^0.1.2",
    "cookie-signature": "^1.0.6",
    "express": "4.12.3",
    "express-session": "^1.11.1",
    "format-message": "2.3.0",
    "intl": "0.1.4",
    "node-fetch": "1.0.6",
    "obj-eql": "^1.0.1",
    "path-to-regexp": "1.0.3",
    "react": "0.13.1",
    "session-file-store": "0.0.3",
    "uniflow": "1.1.0",
    "uniflow-component": "2.0.1"
  },
  "devDependencies": {
    "babel-core": "5.0.12",
    "babel-loader": "5.0.0",
    "css-loader": "0.10.1",
    "cssnext-loader": "1.0.1",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "0.5.0",
    "file-loader": "0.8.1",
    "forever": "0.14.1",
    "format-message-loader": "2.3.0",
    "style-loader": "0.10.1",
    "url-loader": "0.5.5",
    "webpack": "1.8.4",
    "webpack-dev-server": "1.8.0",
    "whatwg-fetch": "0.7.0"
  },
  "scripts": {
    "build": "webpack --colors",
    "build:dev": "webpack -d --colors",
    "extract": "format-message extract -l en -o locales/en.json src/**/*.js",
    "start": "forever start --uid \"iso-react-games\" --minUptime 1000 --spinSleepTime 50 -a src/server.js",
    "stop": "forever stop \"iso-react-games\"",
    "watch": "webpack -d --colors --watch & forever --minUptime 1000 --spinSleepTime 50 -w --watchDirectory src src/server.js"
  },
  "author": "Andy VanWagoner <thetalecrafter@gmail.com>",
  "license": "MIT"
}
